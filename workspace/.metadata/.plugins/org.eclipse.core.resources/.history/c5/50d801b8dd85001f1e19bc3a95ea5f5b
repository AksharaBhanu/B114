package day08;

public class Demo6 {

	public static void main(String[] args) {
		int a;
		
		a=10;
		
		//----------------------------
		if(a>5)
		{
			System.out.println("Hi1");
		}
		
		if(a>7)
		{
			System.out.println("Hi2");
		}
		
		if(a>9)
		{
			System.out.println("Hi3");
		}
		
		//----------------------------
				if(a>5)
				{
					System.out.println("Hi1");
				}
				
				else if(a>7)
				{
					System.out.println("Hi2");
				}
				
				else if(a>9)
				{
					System.out.println("Hi3");
				}
	}
}
