package day09;

public class Demo11 {

	public static void main(String[] args) {
		double i=1;
		
		if(i==1)
		{
			System.out.println("It is One");
		}
		else
		{
			System.out.println("it is not One");
		}

		switch(i)
		{
		case 1: 
				System.out.println("It is One");
				break;
				
		default:
				System.out.println("it is not One");
		}
	}

}
