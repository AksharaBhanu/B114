package day08;

public class Demo5 {
//write a java code to compare 2 variable 
//input a & b
//	output --> 'a is >'     'b is >'   'both are equal'
 	public static void main(String[] args) {
	int a=10,b=20;
	
	if(a>b)
	{
		System.out.println("a is >");
	}
	else if(b>a)
	{
		System.out.println("b is >");
	}
	else
	{
		System.out.println("both are equal");
	}
		
	}

}
