package day05;

public class Demo1 {

	public static void main(String[] args) {
		byte a;
		a=0;
		System.out.println(a);
		
		a=97;
		System.out.println(a);
		
		a=127;
		System.out.println(a);
		
//		a=128;
//		System.out.println(a);
		
		short b=128;
		System.out.println(b);
		
		b=32767;
		System.out.println(b);
		
//		b=32768;
//		System.out.println(b);

	}

}
